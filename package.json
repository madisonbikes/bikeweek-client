    "globals": "^16.1.0",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "knip": "^5.55.1",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
