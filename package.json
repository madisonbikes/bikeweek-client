    "@mui/material": "^7.0.1",
    "@mui/x-data-grid": "^8.3.0",
    "@mui/x-date-pickers": "^8.3.0",
    "@react-oauth/google": "^0.12.2",
    "@tanstack/react-query": "^5.76.0",
    "@tanstack/react-query-devtools": "^5.76.0",
    "@tinymce/tinymce-react": "^6.1.0",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
