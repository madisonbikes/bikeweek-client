{
  "name": "bikeweek-client",
  "version": "1.2.1",
  "scripts": {
    "start": "vite",
    "compile": "tsc --build",
    "build": "npm run compile && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "fmt": "prettier --write .",
    "check-package-updates": "npm-check-updates",
    "lint": "eslint .",
    "prepare": "husky",
    "knip": "knip",
    "check": "npm run build && npm run lint && npm run knip && npm run test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^4.1.3",
